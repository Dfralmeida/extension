{
  "name": "Passagens aéreas melhores preços Genghis Labs",
  "short_name": "Genghis Labs",
  "version": "1.0.5",
  "manifest_version": 2,
  "description": "Busque as melhores passagens aéreas para várias cidades e vários dias de uma só vez. Rápido, seguro e o melhor: sem pagar nada!",
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "background": {
    "scripts": ["js/variables.js", "src/bg/storeManager.js"],
    "persistent": true
  },
  "browser_action": {
    "default_icon": "images/icon19.png",
    "default_title": "Buscar Passagens",
    "default_popup": "src/popup/popup.html"
  },
  "permissions": [
    "*://*/*"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}